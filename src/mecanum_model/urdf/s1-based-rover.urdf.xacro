<?xml version="1.0"?>
<robot name="rover" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:property name="PI" value="3.1415926535897931" />
  <xacro:property name="wheel_radius" value="0.050" />
  <xacro:property name="wheel_width" value="0.0505" />

  <material name="black"><color rgba="0.15 0.15 0.15 1.0" /></material>
  <material name="light_grey"><color rgba="0.4 0.4 0.4 1.0" /></material>

   <xacro:macro name="box_inertia" params="m w h d">
    <inertial>
      <origin xyz="0 0 0" rpy="${pi/2} 0 ${pi/2}"/>
      <mass value="${m}"/>
      <inertia ixx="${(m/12) * (h*h + d*d)}" ixy="0.0" ixz="0.0" iyy="${(m/12) * (w*w + d*d)}" iyz="0.0" izz="${(m/12) * (w*w + h*h)}"/>
    </inertial>
  </xacro:macro>

  <xacro:macro name="cylinder_inertia" params="m r h">
    <inertial>
      <origin xyz="0 0 0" rpy="${pi/2} 0 0" />
      <mass value="${m}"/>
      <inertia ixx="${(m/12) * (3*r*r + h*h)}" ixy = "0" ixz = "0" iyy="${(m/12) * (3*r*r + h*h)}" iyz = "0" izz="${(m/2) * (r*r)}"/>
    </inertial>
  </xacro:macro>

  <xacro:macro name="sphere_inertia" params="m r">
    <inertial>
      <mass value="${m}"/>
      <inertia ixx="${(2/5) * m * (r*r)}" ixy="0.0" ixz="0.0" iyy="${(2/5) * m * (r*r)}" iyz="0.0" izz="${(2/5) * m * (r*r)}"/>
    </inertial>
  </xacro:macro>

  <xacro:include filename="$(find mecanum_model)/urdf/s1-based-rover.gazebo" />

  <link name="world"></link>

  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="base_footprint"/>
  </joint>

  <link name="base_footprint">
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	      <box size="${244.865616 * 0.001} ${302.041534 * 0.001} ${102.041229 * 0.001}"/>
      </geometry>
    </collision>
  </link>

  <joint name="chassis_joint" type="fixed">
    <origin xyz="0 0 0.0" rpy="0 0 0" />
    <parent link="base_footprint"/>
    <child link="chassis" />
 </joint>

  <link name="chassis">
    <visual>
      <geometry>
        <mesh filename="$(find mecanum_model)/meshes/chassis.stl" scale="0.001 0.001 0.001"/>
       </geometry>
       <material name="black" />
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	      <box size="${148.399536 * 0.001} ${252.206665 * 0.001} ${55.450001 * 0.001}"/>
      </geometry>
    </collision>
    <xacro:box_inertia m="1.0" w="${148.399536 * 0.001}" d="${252.206665 * 0.001}" h="${55.450001 * 0.001}"/>
  </link>

  <joint name="sprayer_joint" type="fixed">
   <origin xyz="-0.003 -0.14 0.005" rpy="0 0 0" />
   <parent link="chassis"/>
   <child link="sprayer_assembly" />
 </joint>

 <link name="sprayer_assembly">
    <visual>
      <geometry>
        <mesh filename="$(find mecanum_model)/meshes/sprayer_assembly.stl" scale="0.001 0.001 0.001"/>
       </geometry>
       <material name="light_grey" />
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="${144.179398 * 0.001} ${69.498428 * 0.001} ${96.728165 * 0.001}"/>
      </geometry>
    </collision>
    <xacro:box_inertia m="1.0" w="${144.179398 * 0.001}" d="${69.498428 * 0.001}" h="${96.728165 * 0.001}"/>
  </link>

  <joint name="wheel1_shaft_joint" type="continuous">
     <origin xyz="-0.065 0.1 0.0" rpy="0 0 0" />

      <axis xyz="1 0 0"/>

     <parent link="chassis"/>
     <child link="wheel1_shaft" />
  </joint>

  <link name="wheel1_shaft">
   <visual>
     <origin xyz="0 0 0.0" rpy="0 0 0" />
     <geometry>
       <mesh filename="$(find mecanum_model)/meshes/wheel_shaft_min.stl" scale="0.001 0.001 0.001"/>
     </geometry>
     <material name="black" />
   </visual>
   <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="${32.400002 * 0.001} ${37.585724 * 0.001} ${37.599998 * 0.001}"/>
      </geometry>
    </collision>
    <xacro:box_inertia m="1.0" w="${32.400002 * 0.001}" d="${37.585724 * 0.001}" h="${37.599998 * 0.001}"/>
 </link>

 <joint name="wheel1_wheel_joint" type="fixed">
   <origin xyz="-0.035 0 0" rpy="0 0 0" />

   <parent link="wheel1_shaft"/>
   <child link="wheel1" />
 </joint>

  <link name="wheel1">
