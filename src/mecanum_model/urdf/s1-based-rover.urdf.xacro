      	<mesh filename="$(find mecanum_model)/meshes/wheel_cw.stl" scale="0.001 0.001 0.001"/>
	     </geometry>
       <material name="light_grey" />
    </visual>
     <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="${10  * 0.001} ${10* 0.001} ${102.040993 * 0.001}"/>
      </geometry>
    </collision>
    <xacro:box_inertia m="1.0" w="${45.538025 * 0.001}" d="${102.041016 * 0.001}" h="${102.040993 * 0.001}"/>
  </link>

