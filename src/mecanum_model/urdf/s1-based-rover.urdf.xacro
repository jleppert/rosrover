<?xml version="1.0"?>
<robot name="rover" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:property name="PI" value="3.1415926535897931" />
  <xacro:property name="wheel_radius" value="0.050" />
  <xacro:property name="wheel_width" value="0.0505" />

  <material name="black"><color rgba="0.15 0.15 0.15 1.0" /></material>
  <material name="light_grey"><color rgba="0.4 0.4 0.4 1.0" /></material>

  <link name="base_footprint"></link>

  <joint name="base_link_joint" type="fixed">
    <origin xyz="0 0 0.0" rpy="0 0 0" />
    <parent link="base_footprint"/>
    <child link="base_link" />
 </joint>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://urdf_tutorial/meshes/new/chassis.stl" scale="0.001 0.001 0.001"/>
       </geometry>
       <material name="black" />
    </visual>
  </link>

  <joint name="sprayer_joint" type="fixed">
   <origin xyz="-0.003 -0.14 0.005" rpy="0 0 0" />

   <parent link="base_link"/>
   <child link="sprayer_assembly" />
 </joint>

 <link name="sprayer_assembly">
    <visual>
      <geometry>
        <mesh filename="package://urdf_tutorial/meshes/new/sprayer_assembly.stl" scale="0.001 0.001 0.001"/>
       </geometry>
       <material name="light_grey" />
    </visual>
  </link>

  <joint name="wheel1_shaft_joint" type="continuous">
     <origin xyz="-0.065 0.1 0.0" rpy="0 0 0" />

      <axis xyz="0 1 0"/>

     <parent link="base_link"/>
     <child link="wheel1_shaft" />
  </joint>

  <link name="wheel1_shaft">
   <visual>
     <origin xyz="0 0 0.0" rpy="0 0 0" />
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft.stl" scale="0.001 0.001 0.001"/>
     </geometry>
     <material name="black" />
   </visual>
   <collision>
     <origin xyz="0 0 0.0"/>
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft.stl" scale="0.001 0.001 0.001"/>
     </geometry>
   </collision>
   <inertial>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <mass value="0.055"/>
     <inertia
       ixx="0.000021370" ixy="0" ixz="0"
       iyy="0.000009966" iyz="0"
       izz="0.000021370"/>
   </inertial>
 </link>

 <joint name="wheel1_wheel_joint" type="fixed">
   <origin xyz="-0.035 0 0" rpy="0 0 0" />

   <parent link="wheel1_shaft"/>
   <child link="wheel1" />
 </joint>

  <link name="wheel1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
      	<mesh filename="package://urdf_tutorial/meshes/new/wheel_cw.stl" scale="0.001 0.001 0.001"/>
	     </geometry>
       <material name="light_grey" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.3844"/>
      <!-- Inertia based on cylinder -->
      <inertia
        ixx="0.000324824" ixy="0" ixz="0"
        iyy="0.000480000" iyz="0"
        izz="0.000324824"/>
    </inertial>
  </link>

    <joint name="wheel2_shaft_joint" type="continuous">
     <origin xyz="0.065 0.1 0.0" rpy="0 0 0" />

      <axis xyz="0 1 0"/>
      
     <parent link="base_link"/>
     <child link="wheel2_shaft" />
  </joint>

  <link name="wheel2_shaft">
   <visual>
     <origin xyz="0 0 0.0" rpy="0 0 0" />
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft_max.stl" scale="0.001 0.001 0.001"/>
     </geometry>
     <material name="black" />
   </visual>
   <collision>
     <origin xyz="0 0 0.0"/>
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft_max.stl" scale="0.001 0.001 0.001"/>
     </geometry>
   </collision>
   <inertial>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <mass value="0.055"/>
     <inertia
       ixx="0.000021370" ixy="0" ixz="0"
       iyy="0.000009966" iyz="0"
       izz="0.000021370"/>
   </inertial>
 </link>

 <joint name="wheel2_wheel_joint" type="fixed">
   <origin xyz="0.035 0 0" rpy="0 0 0" />

   <parent link="wheel2_shaft"/>
   <child link="wheel2" />
 </joint>

  <link name="wheel2">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
      	<mesh filename="package://urdf_tutorial/meshes/new/wheel_ccw.stl" scale="0.001 0.001 0.001"/>
	     </geometry>
       <material name="light_grey" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.3844"/>
      <!-- Inertia based on cylinder -->
      <inertia
        ixx="0.000324824" ixy="0" ixz="0"
        iyy="0.000480000" iyz="0"
        izz="0.000324824"/>
    </inertial>
  </link>

   <joint name="wheel3_shaft_joint" type="continuous">
     <origin xyz="0.065 -0.1 0.0" rpy="0 0 0" />

      <axis xyz="0 1 0"/>
      
     <parent link="base_link"/>
     <child link="wheel3_shaft" />
  </joint>

  <link name="wheel3_shaft">
   <visual>
     <origin xyz="0 0 0.0" rpy="0 0 0" />
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft_max.stl" scale="0.001 0.001 0.001"/>
     </geometry>
     <material name="black" />
   </visual>
   <collision>
     <origin xyz="0 0 0.0"/>
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft_max.stl" scale="0.001 0.001 0.001"/>
     </geometry>
   </collision>
   <inertial>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <mass value="0.055"/>
     <inertia
       ixx="0.000021370" ixy="0" ixz="0"
       iyy="0.000009966" iyz="0"
       izz="0.000021370"/>
   </inertial>
 </link>

 <joint name="wheel3_wheel_joint" type="fixed">
   <origin xyz="0.035 0 0" rpy="0 0 0" />

   <parent link="wheel3_shaft"/>
   <child link="wheel3" />
 </joint>

  <link name="wheel3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
      	<mesh filename="package://urdf_tutorial/meshes/new/wheel_cw.stl" scale="0.001 0.001 0.001"/>
	     </geometry>
       <material name="light_grey" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.3844"/>
      <!-- Inertia based on cylinder -->
      <inertia
        ixx="0.000324824" ixy="0" ixz="0"
        iyy="0.000480000" iyz="0"
        izz="0.000324824"/>
    </inertial>
  </link>

   <joint name="wheel4_shaft_joint" type="continuous">
     <origin xyz="-0.065 -0.1 0.0" rpy="0 0 0" />

      <axis xyz="0 1 0"/>
      
     <parent link="base_link"/>
     <child link="wheel4_shaft" />
  </joint>

  <link name="wheel4_shaft">
   <visual>
     <origin xyz="0 0 0.0" rpy="0 0 0" />
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft_min.stl" scale="0.001 0.001 0.001"/>
     </geometry>
     <material name="black" />
   </visual>
   <collision>
     <origin xyz="0 0 0.0"/>
     <geometry>
       <mesh filename="package://urdf_tutorial/meshes/new/wheel_shaft_min.stl" scale="0.001 0.001 0.001"/>
     </geometry>
   </collision>
   <inertial>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <mass value="0.055"/>
     <inertia
       ixx="0.000021370" ixy="0" ixz="0"
       iyy="0.000009966" iyz="0"
       izz="0.000021370"/>
   </inertial>
 </link>

 <joint name="wheel4_wheel_joint" type="fixed">
   <origin xyz="-0.035 0 0" rpy="0 0 0" />

   <parent link="wheel4_shaft"/>
   <child link="wheel4" />
 </joint>

  <link name="wheel4">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
      	<mesh filename="package://urdf_tutorial/meshes/new/wheel_ccw.stl" scale="0.001 0.001 0.001"/>
	     </geometry>
       <material name="light_grey" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="0.3844"/>
      <!-- Inertia based on cylinder -->
      <inertia
        ixx="0.000324824" ixy="0" ixz="0"
        iyy="0.000480000" iyz="0"
        izz="0.000324824"/>
    </inertial>
  </link>
</robot>
